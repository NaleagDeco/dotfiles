# Source common files
source ~/.zshrc.common

#Upload music to work machine
alias bcsi_music_sync="rsync --delete-after --exclude \"lost+found\" -avHAX /srv/music/* gaelan@work:Music"

# Audio conversion
alias rip_to_flac="cdparanoia -B && flac --best -V *.wav && rm *.wav"
alias flac_to_ogg="oggenc -q 6 *.flac"

